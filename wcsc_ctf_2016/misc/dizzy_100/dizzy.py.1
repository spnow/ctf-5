def makeMeDizzy(flag):
    cFlag = ''
    for c in flag:
        cv = ord(c) << 13
        cv = cv >> 4
        cv = cv << 1
        cv = cv >> 2
        cv = cv << 4
        cv = cv >> 4
        cv = cv << 5
        cv = cv >> 13
        cv = cv << 10
        cv = cv >> 4
        cv = cv << 7
        cv = cv >> 12
        cv += 1
        cFlag += chr(cv)
    
    f = open('dizzy_flag','wb')
    f.write(cFlag)
        
if __name__ == '__main__':
    flag = '' # This is almost a gimme
    makeMeDizzy(flag)
